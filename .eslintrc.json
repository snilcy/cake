{
  "env": {
    "es2021": true,
    "node": true,
    "browser": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "extends": ["@snilcy/eslint-config/node"],
  "rules": {
    "node/file-extension-in-import": 0,
    "node/callback-return": 0,
    "node/no-unsupported-features/es-syntax": 0
  }
}
